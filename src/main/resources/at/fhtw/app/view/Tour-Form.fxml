    <children>
        <Label layoutX="46.0" layoutY="86.0" text="Name:"/>
        <TextField fx:id="formName" layoutX="148.0" layoutY="81.0"/>
        <Label layoutX="46.0" layoutY="135.0" text="Description:"/>
        <TextField fx:id="formDescription" layoutX="148.0" layoutY="130.0"/>
